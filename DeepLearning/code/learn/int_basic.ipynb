   "source": [
    "def f(x):\n",
    "    return x**2\n",
    "def df(x, h=1e-10):\n",
    "    return (f(x+h) - f(x-h)) / (2*h)\n",
    "def df_xps(x, h=1e-10):\n",
    "    return (f(x+h) + 4*f(x)+f(x-h))*h/3"
   ]
  },
